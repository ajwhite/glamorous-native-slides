export default function CustomText ({style, children, ...props}) {
  return (
    <Text
      // forward styles
      style={[styles.defaultText, style]}

      // forward props
      {...props}
    >
      {children}
    </Text>
  )
}

const styles = StyleSheet.create({
  defaultText: {
    fontSize: 14,
    color: '#000',
    fontFamily: '...'
  }
})
