function getUsers() {
    return [
        {name: 'Tom', age: 19},
        {name: 'Joe', age: 25},
        {name: 'Bob', age: 30},
        {name: 'Jim', age: 30}
    ]
}

function getUsersByAge (age) {
    return getUsers().filter(user => user.age >= age)
}

getUsersByAge(30)
// [{name: 'Bob', age: 30}, {name: 'Jim', age: 30}]